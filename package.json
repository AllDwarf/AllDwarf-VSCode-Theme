{
  "name": "cyberalldwarf",
  "displayName": "AllDwarf's Color Themes",
  "description": "Azure Data Studio color theme based on popular VS Dark+ and Light+ themes.",
  "version": "0.0.4",
  "publisher": "MarekAldorf",
  "icon": "assets/icon.ico",
  "provides": [
    "AllDwarf's Ironforge Dark",
    "AllDwarf's Ironforge Light"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AllDwarf/cyberalldwarf"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "AllDwarf's Ironforge Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/AllDwarf's Ironforge-dark-color-theme.json"
      },
      {
        "label": "AllDwarf's Ironforge Light",
        "uiTheme": "vs",
        "path": "./themes/AllDwarf's Ironforge-color-theme.json"
      }
    ]
  },
  "scripts": {
    "package": "npx vsce package --packagePath release"
  }
}