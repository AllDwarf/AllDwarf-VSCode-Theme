{
  "name": "cyberalldwarf",
  "displayName": "CyberAllDwarf",
  "description": "Azure Data Studio color theme based on popular Cyberpunk theme",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/AllDwarf/cyberalldwarf"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "AllDwarf's Cyberpunk",
        "uiTheme": "vs-dark",
        "path": "./themes/AllDwarf's Cyberpunk-color-theme.json"
      },
      {
        "label": "AllDwarf's Ironforge",
        "uiTheme": "vs",
        "path": "./themes/AllDwarf's Ironforge-color-theme.json"
      }
    ]
